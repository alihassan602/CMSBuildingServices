{
  "name": "learn-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start"
  },
  "dependencies": {
    "antd": "^4.6.1",
    "next": "9.3.5",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "prettier": "2.1.1"
  },
  "prettier": {
    "tabWidth": 4,
    "singleQuote": true,
    "useTabs": true,
    "semi": false,
    "overrides": [
      {
        "files": [
          "*.yml",
          "*.yaml"
        ],
        "options": {
          "useTabs": false,
          "tabWidth": 2
        }
      },
      {
        "files": [
          "package.json",
          "server/package.json"
        ],
        "options": {
          "useTabs": false,
          "tabWidth": 2
        }
      }
    ]
  }
}
